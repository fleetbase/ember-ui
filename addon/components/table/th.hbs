<Table::Cell 
    @inHead={{true}} 
    @resizable={{@resizable}} 
    @sortable={{@sortable}} 
    @isSorted={{this.isSorted}}
    @isAscending={{this.isAscending}}
    @isDescending={{this.isDescending}}
    @sortPriority={{this.sortPriority}}
    @showSortPriority={{this.showSortPriority}}
    {{on "click" this.onClick}} 
    {{did-insert this.setupComponent}} 
    ...attributes
>
    {{yield}}
</Table::Cell>
